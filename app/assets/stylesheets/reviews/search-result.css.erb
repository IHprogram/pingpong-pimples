.result-main {
  padding: 40px 0;
  background-image: url("<%= asset_path("back-img.jpg") %>");
  background-size: cover;
  display: flex;
  justify-content: center;
  flex-direction: column;
  padding: 120px 0 50px;
}

.result-title-wrapper {
  text-align: center;
  margin: 0 auto 40px;
}

.result-title {
  font-size: 25px;
  font-weight: bold;
}

.top-page-back {
  height: 100px;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.back-btn {
  background-color: #aaaaaa;
  color: #ffffff;
  text-decoration: none;
  border-radius: 30px;
  padding: 15px 80px;
}
